            <Label Content="mm" Style="{StaticResource MyLabelHeaderStyle}" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="104,3,9,7" Width="56" RenderTransformOrigin="1.125,0.563" Height="40" Visibility="Hidden"/>
