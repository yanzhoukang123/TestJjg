        <TextBox  Grid.Row="42" Grid.Column="2"
                    x:Name="oldVesion_txtSerialNumber"
                    Width="80"
                    Height="25"
                    FontSize="14"
                    FontWeight="SemiBold"
                    HorizontalContentAlignment="Center"
                    VerticalContentAlignment="Center"
                    Text="{Binding Path=SystemSN, Mode=TwoWay, TargetNullValue={x:Static sys:String.Empty}}" Margin="7,2,3,2"/>
        <Grid  Grid.Row="50" Grid.Column="1"  HorizontalAlignment="Right" VerticalAlignment="Center" Grid.ColumnSpan="2" Margin="0,2,11,2"  Visibility="{Binding LEDVesionVisFlag}">
            <Label   Content="SN" Visibility="{Binding VesionVisFlag}" Margin="6,0,90,0"/>
        </Grid>
        <Grid  Grid.Row="50" Grid.Column="2"  HorizontalAlignment="Right" VerticalAlignment="Center" Grid.ColumnSpan="2" Margin="0,2,11,2"  Visibility="{Binding LEDVesionVisFlag}">
            <TextBox
                    Width="80"
                    Height="25"
                    FontSize="14"
                    FontWeight="SemiBold"
                    HorizontalContentAlignment="Center"
                    VerticalContentAlignment="Center"
                    Text="{Binding Path=SystemSN, Mode=TwoWay, TargetNullValue={x:Static sys:String.Empty}}" Margin="5,2" Visibility="{Binding VesionVisFlag}" />
        </Grid>
        <Grid  Grid.Row="42" Grid.Column="0" HorizontalAlignment="Right" VerticalAlignment="Center" Grid.ColumnSpan="2" Margin="0,4,9,4"  Visibility="{Binding LEDVesionVisFlag}">
            <Label  Content="Back Fan Speed Level " Visibility="{Binding VesionVisFlag}"/>
        </Grid>
        <Grid  Grid.Row="42" Grid.Column="2" Visibility="{Binding LEDVesionVisFlag}">
            <ComboBox x:Name="_CoefficientModule1"
                  Visibility="{Binding VesionVisFlag}"
                  
                        Width="80"
                        Height="25"
                        Style="{StaticResource ComboBoxFlatStyle}"
                        FontSize="14"
                        FontWeight="SemiBold"
                        ItemsSource="{Binding IncrustationOptions}" 
                        SelectedItem="{Binding SelectedIncrustation}"
                        Margin="4,4,4,3" VerticalContentAlignment="Center" />
        </Grid>
        <Grid  Grid.Row="44" Grid.Column="0"  HorizontalAlignment="Right" VerticalAlignment="Center" Grid.ColumnSpan="2" Margin="0,2,11,2"  Visibility="{Binding LEDVesionVisFlag}">
            <Label Content="Default Level" Visibility="{Binding VesionVisFlag}"/>
        </Grid>
        <Grid  Grid.Row="44" Grid.Column="2" Visibility="{Binding LEDVesionVisFlag}">
            <TextBox 
                    Width="80"
                    Height="25"
                    FontSize="14"
                    FontWeight="SemiBold"
                    HorizontalContentAlignment="Center"
                    VerticalContentAlignment="Center"
                    PreviewTextInput="TextBox_PreviewTextInput"
                     Visibility="{Binding VesionVisFlag}"
                    Text="{Binding Path=ShellFanDefaultSpeed, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay, TargetNullValue={x:Static sys:String.Empty}}" Margin="6,2,2,2"/>
        </Grid>
        <Grid  Grid.Row="46" Grid.Column="0"  HorizontalAlignment="Right" VerticalAlignment="Center" Grid.ColumnSpan="2" Margin="0,4,9,4" Visibility="{Binding LEDVesionVisFlag}">
            <Label  Content="Cabinet Temperature Control" Visibility="{Binding VesionVisFlag}"/>
        </Grid>
        <Grid Grid.Row="46" Grid.Column="2" Visibility="{Binding LEDVesionVisFlag}">
            <ComboBox x:Name="_CH1AlertWarning"
                  Visibility="{Binding VesionVisFlag}"
                       Width="80"
                    Height="25"
                        Style="{StaticResource ComboBoxFlatStyle}"
                        FontSize="14"
                        FontWeight="SemiBold"
                        ItemsSource="{Binding CH1AlertWarningOptions}" 
                        SelectedItem="{Binding SelectedCH1AlertWarning}"
                        Margin="4,4,4,3" VerticalContentAlignment="Center" />
        </Grid>
        <Grid  Grid.Row="48" Grid.Column="0"  VerticalAlignment="Center" Grid.ColumnSpan="2" Margin="0,2,11,2" Visibility="{Binding CH1AlertFlag}">
            <Grid Visibility="{Binding LEDVesionVisFlag}">
                <Label Content="Warning Temperature(℃)" HorizontalAlignment="Right" Visibility="{Binding VesionVisFlag}"/>
            </Grid>
        </Grid>
        <Grid Grid.Row="48" Grid.Column="2" Visibility="{Binding CH1AlertFlag}">
            <Grid Visibility="{Binding LEDVesionVisFlag}">
                <TextBox
                    Width="80"
                    Height="25"
                    FontSize="14"
                    FontWeight="SemiBold"
                    HorizontalContentAlignment="Center"
                    VerticalContentAlignment="Center"
                    PreviewTextInput="TextBox_PreviewTextInput"
                     Visibility="{Binding VesionVisFlag}"
                    Text="{Binding Path=CH1WarningTemperature, UpdateSourceTrigger=LostFocus, Mode=TwoWay, TargetNullValue={x:Static sys:String.Empty}}" Margin="6,2,2,2"/>
            </Grid>
        </Grid>
        <Label Grid.Row="0" Grid.Column="4" Content="L             All" HorizontalAlignment="Right" VerticalAlignment="Center"  Margin="0,2,3,2"/>
        <CheckBox Grid.Row="0" Grid.Column="6"  IsChecked="{Binding Path=IsLCheckALL}" HorizontalAlignment="Center"></CheckBox>
        <Label Grid.Row="2" Grid.Column="4" Content="FVN" HorizontalAlignment="Right" VerticalAlignment="Center"  Margin="0,2,3,2"/>
        <TextBox Grid.Row="2" Grid.Column="6"
                    IsReadOnly="True"
                    Width="80"
                    Height="25"
                    FontSize="14"
                    FontWeight="SemiBold"
                    HorizontalContentAlignment="Center"
                    VerticalContentAlignment="Center"
                    Text="{Binding Path=LIVFirmwareVersionSN, Mode=TwoWay, TargetNullValue={x:Static sys:String.Empty}}" Margin="7,2,3,2"/>
