                    Height="25"
                    FontSize="14"
                    FontWeight="SemiBold"
                    HorizontalContentAlignment="Center"
                    VerticalContentAlignment="Center"
                    IsEnabled="{Binding IsEnabledL1}"
                    Text="{Binding Path=LTEControlTemperature,StringFormat=0.0, Mode=TwoWay, TargetNullValue={x:Static sys:String.Empty}}"  Margin="7,1,3,1"/>
