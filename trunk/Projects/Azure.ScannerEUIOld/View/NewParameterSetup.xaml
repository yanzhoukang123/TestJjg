                    Height="25"
                    FontSize="14"
                    FontWeight="SemiBold"
                    HorizontalContentAlignment="Center"
                    VerticalContentAlignment="Center"
                    Text="{Binding Path=XEncoderSubdivision, Mode=TwoWay, TargetNullValue={x:Static sys:String.Empty}}" Margin="6,2,2,2"/>
