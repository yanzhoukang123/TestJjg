        <CheckBox Grid.Row="6" Grid.Column="7"  IsEnabled="{Binding IsEnabledL1}" IsChecked="{Binding Path=IsLWavelength}"></CheckBox>
        <TextBox Grid.Row="6" Grid.Column="6"
                    Width="80"
                    Height="25"
                    FontSize="14"
                    FontWeight="SemiBold"
                    HorizontalContentAlignment="Center"
                    VerticalContentAlignment="Center"
                    IsEnabled="{Binding IsEnabledL1}"
                    Text="{Binding Path=LWavelength, Mode=TwoWay, TargetNullValue={x:Static sys:String.Empty}}"  Margin="7,1,3,1"/>
