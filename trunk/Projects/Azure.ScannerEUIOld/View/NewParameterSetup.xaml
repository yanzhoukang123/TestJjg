                    HorizontalContentAlignment="Center"
                    VerticalContentAlignment="Center"
                    Text="{Binding Path=LIVFirmwareSN, Mode=TwoWay, TargetNullValue={x:Static sys:String.Empty}}" Margin="7,1,3,1"/>
