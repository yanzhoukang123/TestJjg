        <CheckBox Grid.Row="10" Grid.Column="7" IsEnabled="{Binding IsEnabledL1}" IsChecked="{Binding Path=IsLTECMaximumCoolingCurrent}"></CheckBox>
        <TextBox Grid.Row="10" Grid.Column="6"
                    Width="80"
                    Height="25"
                    FontSize="14"
                    FontWeight="SemiBold"
                    HorizontalContentAlignment="Center"
                    VerticalContentAlignment="Center"
                    IsEnabled="{Binding IsEnabledL1}"
                    Text="{Binding Path=LTECMaximumCoolingCurrent, Mode=TwoWay, TargetNullValue={x:Static sys:String.Empty}}" Margin="7,1,3,1"/>
