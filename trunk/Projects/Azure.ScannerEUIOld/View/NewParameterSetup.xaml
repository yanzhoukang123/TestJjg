        <TextBox Grid.Row="20" Grid.Column="2"
                    Width="80"
                    Height="25"
                    FontSize="14"
                    FontWeight="SemiBold"
                    HorizontalContentAlignment="Center"
                    VerticalContentAlignment="Center"
                    PreviewTextInput="TextBox_PreviewTextInput"
                    Text="{Binding Path=Pixel_10_Offset_L_R1, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay, TargetNullValue={x:Static sys:String.Empty}}" Margin="6,1,2,1"/>
