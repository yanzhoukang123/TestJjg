                    IsEnabled="{Binding Path=IsMotorWEnabled}"
                    Command="{Binding Path=GoAbsPosCommand}"
                    CommandParameter="{x:Static imagingsys:MotorType.W}" />
            </StackPanel>-->

            <!-- Relative position-->
            <Label Grid.Row="0" Grid.Column="10"
               Content="Relative(mm)" 
               Style="{StaticResource MyLabelHeaderStyle}"
               Margin="33,2,34,2" Height="29" Width="97" />
            <StackPanel Grid.Row="2" Grid.Column="10" x:Name="_RelativePosX" Orientation="Horizontal">
                <Button x:Name="_RelativeXMinusButton"
                    Content=" - "
                    Style="{DynamicResource BlueButton}"
                    Width="40"
                    Height="26"
                    FontSize="14"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Center"
                    Margin="2,1,1,1"
                    IsEnabled="{Binding Path=IsMotorXEnabled}"
                    Command="{Binding Path=RelativePosMinusCommand}"
                    CommandParameter="{x:Static imagingsys:MotorType.X}" />
                <TextBox x:Name="_RelativePosXTextBox"
                    MinWidth="70"
                    FontSize="14"
                    FontWeight="SemiBold"
                    Margin="2"
                    IsEnabled="{Binding Path=IsMotorXEnabled}"
                    HorizontalContentAlignment="Center"
                    VerticalContentAlignment="Center"
                    PreviewTextInput="TextBox_PreviewTextInput"
                    Text="{Binding Path=RelativeXPos, UpdateSourceTrigger=PropertyChanged, TargetNullValue={x:Static sys:String.Empty}}" />
                <Button x:Name="_RelativeXPlusButton"
                    Content=" + "
                    Style="{DynamicResource BlueButton}"
                    Width="40"
                    Height="26"
                    FontSize="14"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Center"
                    Margin="1,1,2,1"
                    IsEnabled="{Binding Path=IsMotorXEnabled}"
                    Command="{Binding Path=RelativePosPlusCommand}"
                    CommandParameter="{x:Static imagingsys:MotorType.X}" />
            </StackPanel>
            <StackPanel Grid.Row="4" Grid.Column="10" x:Name="_RelativePosY" Orientation="Horizontal">
                <Button x:Name="_RelativeYMinusButton"
                    Content=" - "
                    Style="{DynamicResource BlueButton}"
                    Width="40"
                    Height="26"
                    FontSize="14"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Center"
                    Margin="2,1,1,1"
                    IsEnabled="{Binding Path=IsMotorYEnabled}"
                    Command="{Binding Path=RelativePosMinusCommand}"
                    CommandParameter="{x:Static imagingsys:MotorType.Y}" />
                <TextBox x:Name="_RelativePosYTextBox"
                    MinWidth="70"
                    FontSize="14"
                    FontWeight="SemiBold"
                    HorizontalContentAlignment="Center"
                    VerticalContentAlignment="Center"
                    Margin="2"
                    IsEnabled="{Binding Path=IsMotorYEnabled}"
                    PreviewTextInput="TextBox_PreviewTextInput"
                    Text="{Binding Path=RelativeYPos, UpdateSourceTrigger=PropertyChanged, TargetNullValue={x:Static sys:String.Empty}}" />
                <Button x:Name="_RelativeYPlusButton"
                    Content=" + "
                    Style="{DynamicResource BlueButton}"
                    Width="40"
                    Height="26"
                    FontSize="14"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Center"
                    Margin="1,1,2,1"
                    IsEnabled="{Binding Path=IsMotorYEnabled}"
                    Command="{Binding Path=RelativePosPlusCommand}"
                    CommandParameter="{x:Static imagingsys:MotorType.Y}" />
            </StackPanel>
            <StackPanel Grid.Row="6" Grid.Column="10" x:Name="_RelativePosZ" Orientation="Horizontal">
                <Button x:Name="_RelativeZPosMinusButton"
                    Content=" - "
                    Style="{DynamicResource BlueButton}"
                    Width="40"
                    Height="26"
                    FontSize="14"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Center"
                    Margin="2,1,1,1"
                    IsEnabled="{Binding Path=IsMotorZEnabled}"
                    Command="{Binding Path=RelativePosMinusCommand}"
                    CommandParameter="{x:Static imagingsys:MotorType.Z}" />
                <TextBox x:Name="_RelativePosZTextBox"
                    MinWidth="70"
                    FontSize="14"
                    FontWeight="SemiBold"
                    Margin="2"
                    IsEnabled="{Binding Path=IsMotorZEnabled}"
                    HorizontalContentAlignment="Center"
                    VerticalContentAlignment="Center"
                    PreviewTextInput="TextBox_PreviewTextInput"
                    Text="{Binding Path=RelativeZPos, UpdateSourceTrigger=PropertyChanged, TargetNullValue={x:Static sys:String.Empty}}" />
                <Button x:Name="_RelativeZPlusButton"
                    Content=" + "
                    Style="{DynamicResource BlueButton}"
                    Width="40"
                    Height="26"
                    FontSize="14"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Center"
                    Margin="1,1,2,1"
                    IsEnabled="{Binding Path=IsMotorZEnabled}"
                    Command="{Binding Path=RelativePosPlusCommand}"
                    CommandParameter="{x:Static imagingsys:MotorType.Z}" />
            </StackPanel>
