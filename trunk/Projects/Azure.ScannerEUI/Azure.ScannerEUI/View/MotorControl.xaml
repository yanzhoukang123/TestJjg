<UserControl x:Class="Azure.ScannerEUI.View.MotorControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             xmlns:ToggleSwitch="clr-namespace:ToggleSwitch;assembly=ToggleSwitch"
             xmlns:local="clr-namespace:Azure.ScannerEUI.ViewModel"
             xmlns:imagingsys="clr-namespace:Azure.ImagingSystem;assembly=Azure.ImagingSystem"
             mc:Ignorable="d"
             DataContext="{Binding Path=MotorVM}" Height="146.509" Width="995">

    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Azure.ScannerEUI;component/Resources/AllResources.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <Style x:Key="MyLabelHeaderStyle" TargetType="{x:Type Label}">
                <Setter Property="Foreground" Value="DarkBlue" />
                <Setter Property="FontSize" Value="14" />
                <Setter Property="FontFamily" Value="Segoe UI" />
                <Setter Property="FontWeight" Value="Bold" />
                <Setter Property="HorizontalAlignment" Value="Center" />
                <Setter Property="VerticalAlignment" Value="Center" />
            </Style>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid Margin="0,0,7,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto" MinWidth="854" />
            <ColumnDefinition Width="4" />
            <ColumnDefinition Width="Auto" />
        </Grid.ColumnDefinitions>

        <Grid Grid.Column="0">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="4" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="4" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="4" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="4" />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="4" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="4" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="4" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="5" />
                <ColumnDefinition Width="Auto" MinWidth="125" />
                <ColumnDefinition Width="4" />
                <ColumnDefinition Width="Auto" MinWidth="164" />
                <ColumnDefinition Width="5" />
                <ColumnDefinition Width="Auto" MinWidth="100" />
                <ColumnDefinition Width="3" />
                <ColumnDefinition Width="Auto" />
            </Grid.ColumnDefinitions>

            <!--Column1: Label-->
            <Label Grid.Row="2" Grid.Column="0" Content=" X " Style="{StaticResource MyLabelHeaderStyle}" Margin="2,2,7,2" Height="29" Width="28"/>
            <Label Grid.Row="4" Grid.Column="0" Content=" Y " Style="{StaticResource MyLabelHeaderStyle}" Margin="2,2,9,2" Height="29" Width="26"/>
            <Label Grid.Row="6" Grid.Column="0" Content=" Z " Style="{StaticResource MyLabelHeaderStyle}" Margin="2,2,9,2" Height="29" Width="26"/>
            <!--<Label Grid.Row="8" Grid.Column="0" Content=" W " Style="{StaticResource MyLabelHeaderStyle}" Margin="2"/>-->

            <!--Column2: Home buttons-->
            <Label Grid.Row="0" Grid.Column="2"
                Content="Units(mm)"
                Style="{StaticResource MyLabelHeaderStyle}"
                Margin="2" Height="29" Width="80"/>
            <Button Grid.Row="2" Grid.Column="2" x:Name="_MotorXHome"
                Content="Home"
                Style="{DynamicResource BlueButton}"
                Width="80"
                Height="25"
                FontSize="14"
                HorizontalAlignment="Right"
                VerticalAlignment="Center"
                Margin="0,4,2,4"
                IsEnabled="{Binding Path=IsMotorXEnabled}"
                Command="{Binding Path=HomeCommand}"
                CommandParameter="{x:Static imagingsys:MotorType.X}" />
            <Button Grid.Row="4" Grid.Column="2" x:Name="_MotorYHome"
                Content="Home"
                Style="{DynamicResource BlueButton}"
                Width="80"
                Height="25"
                FontSize="14"
                HorizontalAlignment="Right"
                VerticalAlignment="Center"
                Margin="0,4,2,4"
                IsEnabled="{Binding Path=IsMotorYEnabled}"
                Command="{Binding Path=HomeCommand}"
                CommandParameter="{x:Static imagingsys:MotorType.Y}" />
            <Button Grid.Row="6" Grid.Column="2" x:Name="_MotorZHome"
                Content="Home"
                Style="{DynamicResource BlueButton}"
                Width="80"
                Height="25"
                FontSize="14"
                HorizontalAlignment="Right"
                VerticalAlignment="Center"
                Margin="0,4,2,4"
                IsEnabled="{Binding Path=IsMotorZEnabled}"
                Command="{Binding Path=HomeCommand}"
                CommandParameter="{x:Static imagingsys:MotorType.Z}" />
            <!--<Button Grid.Row="8" Grid.Column="2" x:Name="_MotorWHome"
                Content="Home"
                Style="{DynamicResource BlueButton}"
                Width="80"
                Height="25"
                FontSize="14"
                HorizontalAlignment="Right"
                VerticalAlignment="Center"
                Margin="2,1"
                IsEnabled="{Binding Path=IsMotorWEnabled}"
                Command="{Binding Path=HomeCommand}"
                CommandParameter="{x:Static imagingsys:MotorType.W}" />-->

            <!-- Speed setting -->
            <Label Grid.Row="0" Grid.Column="4"
                Content="Speed(mm/s)"
                Style="{StaticResource MyLabelHeaderStyle}"
                Margin="2" Height="29" Width="99"/>
            <TextBox Grid.Row="2" Grid.Column="4"
                x:Name="_SpeedXTextBox"
                MinWidth="70"
                FontSize="14"
                FontWeight="SemiBold"
                Background="LightBlue"
                HorizontalContentAlignment="Center"
                VerticalContentAlignment="Center"
                Margin="2"
                IsEnabled="{Binding Path=IsMotorXEnabled}"
                PreviewTextInput="TextBox_PreviewTextInput"
                PreviewKeyDown="TextBox_PreviewKeyDown"
                Text="{Binding Path=MotorXSpeed, UpdateSourceTrigger=PropertyChanged, TargetNullValue={x:Static sys:String.Empty}}" />
            <TextBox Grid.Row="4" Grid.Column="4"
                x:Name="_SpeedYTextBox"
                MinWidth="70"
                FontSize="14"
                FontWeight="SemiBold"
                Background="LightBlue"
                HorizontalContentAlignment="Center"
                VerticalContentAlignment="Center"
                Margin="2"
                IsEnabled="{Binding Path=IsMotorYEnabled}"
                PreviewTextInput="TextBox_PreviewTextInput"
                PreviewKeyDown="TextBox_PreviewKeyDown"
                Text="{Binding Path=MotorYSpeed, UpdateSourceTrigger=PropertyChanged, TargetNullValue={x:Static sys:String.Empty}}" />
            <TextBox Grid.Row="6" Grid.Column="4"
                x:Name="_SpeedZTextBox"
                MinWidth="70"
                FontSize="14"
                FontWeight="SemiBold"
                Background="LightBlue"
                HorizontalContentAlignment="Center"
                VerticalContentAlignment="Center"
                Margin="2"
                IsEnabled="{Binding Path=IsMotorZEnabled}"
                PreviewTextInput="TextBox_PreviewTextInput"
                PreviewKeyDown="TextBox_PreviewKeyDown"
                Text="{Binding Path=MotorZSpeed, UpdateSourceTrigger=PropertyChanged, TargetNullValue={x:Static sys:String.Empty}}" />
            <!--<TextBox Grid.Row="8" Grid.Column="4"
                x:Name="_SpeedWTextBox"
                MinWidth="70"
                FontSize="14"
                FontWeight="SemiBold"
                Background="LightBlue"
                HorizontalContentAlignment="Center"
                VerticalContentAlignment="Center"
                Margin="2"
                IsEnabled="{Binding Path=IsMotorWEnabled}"
                PreviewTextInput="TextBox_PreviewTextInput"
                PreviewKeyDown="TextBox_PreviewKeyDown"
                Text="{Binding Path=MotorWSpeed, UpdateSourceTrigger=PropertyChanged, TargetNullValue={x:Static sys:String.Empty}}" />-->

            <!--Acceleration-->
            <Label Grid.Row="0" Grid.Column="6"
               Content="Accel(mm/s²)" 
               Style="{StaticResource MyLabelHeaderStyle}"
               HorizontalAlignment="Center"
               Margin="2" Height="29" Width="99" />
            <TextBox Grid.Row="2" Grid.Column="6"
                x:Name="_AccelXTextBox"
                MinWidth="70"
                FontSize="14"
                FontWeight="SemiBold"
                Background="LightBlue"
                HorizontalContentAlignment="Center"
                VerticalContentAlignment="Center"
                Margin="2"
                IsEnabled="{Binding Path=IsMotorXEnabled}"
                PreviewTextInput="TextBox_PreviewTextInput"
                PreviewKeyDown="TextBox_PreviewKeyDown"
                Text="{Binding Path=MotorXAccel, UpdateSourceTrigger=PropertyChanged, TargetNullValue={x:Static sys:String.Empty}}" />
            <TextBox Grid.Row="4" Grid.Column="6"
                x:Name="_AccelYTextBox"
                MinWidth="70"
                FontSize="14"
                FontWeight="SemiBold"
                Background="LightBlue"
                Margin="2"
                IsEnabled="{Binding Path=IsMotorYEnabled}"
                HorizontalContentAlignment="Center"
                VerticalContentAlignment="Center"
                PreviewTextInput="TextBox_PreviewTextInput"
                PreviewKeyDown="TextBox_PreviewKeyDown"
                Text="{Binding Path=MotorYAccel, UpdateSourceTrigger=PropertyChanged, TargetNullValue={x:Static sys:String.Empty}}" />
            <TextBox Grid.Row="6" Grid.Column="6"
                x:Name="_AccelZTextBox"
                MinWidth="70"
                FontSize="14"
                FontWeight="SemiBold"
                Background="LightBlue"
                HorizontalContentAlignment="Center"
                VerticalContentAlignment="Center"
                Margin="2"
                IsEnabled="{Binding Path=IsMotorZEnabled}"
                PreviewTextInput="TextBox_PreviewTextInput"
                PreviewKeyDown="TextBox_PreviewKeyDown"
                Text="{Binding Path=MotorZAccel, UpdateSourceTrigger=PropertyChanged, TargetNullValue={x:Static sys:String.Empty}}" />
            <!--<TextBox Grid.Row="8" Grid.Column="6"
                x:Name="_AccelWTextBox"
                MinWidth="70"
                FontSize="14"
                FontWeight="SemiBold"
                Background="LightBlue"
                HorizontalContentAlignment="Center"
                VerticalContentAlignment="Center"
                Margin="2"
                IsEnabled="{Binding Path=IsMotorWEnabled}"
                PreviewTextInput="TextBox_PreviewTextInput"
                PreviewKeyDown="TextBox_PreviewKeyDown"
                Text="{Binding Path=MotorWAccel, UpdateSourceTrigger=PropertyChanged, TargetNullValue={x:Static sys:String.Empty}}" />-->

            <!-- Absolute position -->
            <Label Grid.Row="0" Grid.Column="8"
               Content="Absolute(mm)" 
               Style="{StaticResource MyLabelHeaderStyle}"
               HorizontalAlignment="Left"
               Margin="8,2,0,2" Height="29" Width="105" />
            <StackPanel Grid.Row="2" Grid.Column="8" x:Name="_AbsolutePosX" Orientation="Horizontal">
                <TextBox x:Name="_AbsolutePosXTextBox"
                    MinWidth="70"
                    FontSize="14"
                    FontWeight="SemiBold"
                    HorizontalContentAlignment="Center"
                    VerticalContentAlignment="Center"
                    Margin="2"
                    IsEnabled="{Binding Path=IsMotorXEnabled}"
                    PreviewTextInput="TextBox_PreviewTextInput"
                    Text="{Binding Path=AbsXPos, UpdateSourceTrigger=PropertyChanged, TargetNullValue={x:Static sys:String.Empty}}" />
                <Button x:Name="_GoXButton"
                    Content="Go"
                    Style="{DynamicResource BlueButton}"
                    Width="45"
                    Height="26"
                    FontSize="14"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Center"
                    Margin="1,1,2,1"
                    IsEnabled="{Binding Path=IsMotorXEnabled}"
                    Command="{Binding Path=GoAbsPosCommand}"
                    CommandParameter="{x:Static imagingsys:MotorType.X}" />
            </StackPanel>
            <StackPanel Grid.Row="4" Grid.Column="8" x:Name="_AbsolutePosY" Orientation="Horizontal">
                <TextBox x:Name="_AbsolutePosYTextBox"
                    MinWidth="70"
                    FontSize="14"
                    FontWeight="SemiBold"
