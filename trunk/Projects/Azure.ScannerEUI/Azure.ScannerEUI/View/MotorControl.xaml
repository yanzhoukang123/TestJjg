                    IsEnabled="{Binding Path=IsMotorYEnabled}"
                    PreviewTextInput="TextBox_PreviewTextInput"
                    Text="{Binding Path=AbsYPos, UpdateSourceTrigger=PropertyChanged, TargetNullValue={x:Static sys:String.Empty}}" />
                <Button x:Name="_GoAbsYButton"
                    Content="Go"
                    Style="{DynamicResource BlueButton}"
                    Width="45"
                    Height="26"
                    FontSize="14"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Center"
                    Margin="1,1,2,1"
                    IsEnabled="{Binding Path=IsMotorYEnabled}"
                    Command="{Binding Path=GoAbsPosCommand}"
                    CommandParameter="{x:Static imagingsys:MotorType.Y}" />
            </StackPanel>
            <StackPanel Grid.Row="6" Grid.Column="8" x:Name="_AbsolutePosZ" Orientation="Horizontal">
                <TextBox x:Name="_AbsolutePosZTextBox"
                    MinWidth="70"
                    FontSize="14"
                    FontWeight="SemiBold"
                    HorizontalContentAlignment="Center"
                    VerticalContentAlignment="Center"
                    Margin="2"
                    IsEnabled="{Binding Path=IsMotorZEnabled}"
                    PreviewTextInput="TextBox_PreviewTextInput"
                    Text="{Binding Path=AbsZPos, TargetNullValue={x:Static sys:String.Empty}}" />
                <Button x:Name="_GoAbsZPosButton"
                    Content="Go"
                    Style="{DynamicResource BlueButton}"
                    Width="45"
                    Height="26"
                    FontSize="14"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Center"
                    Margin="1,1,2,1"
                    IsEnabled="{Binding Path=IsMotorZEnabled}"
                    Command="{Binding Path=GoAbsPosCommand}"
                    CommandParameter="{x:Static imagingsys:MotorType.Z}" />
            </StackPanel>
            <!--<StackPanel Grid.Row="8" Grid.Column="8" x:Name="_AbsolutePosW" Orientation="Horizontal">
                <TextBox x:Name="_AbsolutePosWTextBox"
                    MinWidth="70"
                    FontSize="14"
                    FontWeight="SemiBold"
                    HorizontalContentAlignment="Center"
                    VerticalContentAlignment="Center"
                    Margin="2"
                    IsEnabled="{Binding Path=IsMotorWEnabled}"
                    PreviewTextInput="TextBox_PreviewTextInput"
                    Text="{Binding Path=AbsWPos, TargetNullValue={x:Static sys:String.Empty}}" />
