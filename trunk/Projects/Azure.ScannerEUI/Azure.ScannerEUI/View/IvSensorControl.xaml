        </StackPanel>
        <StackPanel Grid.Column="5"   Grid.Row="2"  Orientation="Horizontal"  HorizontalAlignment="Center" Margin="7,0,77,0" Width="123">
            <TextBox x:Name="_SensorMR2"
                        Width="111"
                        Height="25"
                        FontSize="14"
                        FontWeight="SemiBold"
