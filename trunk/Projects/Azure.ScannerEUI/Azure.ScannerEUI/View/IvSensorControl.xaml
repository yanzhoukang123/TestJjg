        <StackPanel   Grid.Row="4"  Orientation="Horizontal"  HorizontalAlignment="Center" Margin="3,0,4,0" Width="121" Grid.Column="1">

            <TextBox x:Name="_SensorSNL1"
                        Width="111"
                        Height="25"
                        FontSize="14"
                        FontWeight="SemiBold"
                        VerticalContentAlignment="Center"
                        IsReadOnly="{Binding Path=IsReadOnly}"
                        BorderBrush="{x:Null}" BorderThickness="0"
                        Text="{Binding Path=SensorSNL1, Mode=TwoWay, TargetNullValue={x:Static sys:String.Empty}}" Margin="0,2"/>
        </StackPanel>
        <StackPanel Grid.Column="3"   Grid.Row="4"  Orientation="Horizontal"  HorizontalAlignment="Center" Margin="4,0" Width="116">
            <TextBox x:Name="_SensorSNR1"
                        Width="111"
                        Height="25"
                        FontSize="14"
                        FontWeight="SemiBold"
                        VerticalContentAlignment="Center"
                        IsReadOnly="{Binding Path=IsReadOnly}"
                        BorderBrush="{x:Null}" BorderThickness="0"
                        Text="{Binding Path=SensorSNR1, Mode=TwoWay, TargetNullValue={x:Static sys:String.Empty}}" Margin="0,2"/>
        </StackPanel>
        <StackPanel Grid.Column="5"   Grid.Row="4"  Orientation="Horizontal"  HorizontalAlignment="Center" Margin="5,0,86,0" Width="116">
            <TextBox x:Name="_SensorSNR2"
                        Width="111"
                        Height="25"
                        FontSize="14"
                        FontWeight="SemiBold"
                        VerticalContentAlignment="Center"
                        BorderBrush="{x:Null}" BorderThickness="0"
                        IsReadOnly="{Binding Path=IsReadOnly}"
                        Text="{Binding Path=SensorSNR2, Mode=TwoWay, TargetNullValue={x:Static sys:String.Empty}}" Margin="0,2"/>
        </StackPanel>
        <!--LaserSN-->
        <StackPanel Grid.Column="0"   Grid.Row="6"  Orientation="Horizontal"  HorizontalAlignment="Center" Margin="-1,0,34,0" Width="116">
            <Label Content="Laser SN:" Style="{StaticResource MyLabelHeaderStyle}" Width="77" />
        </StackPanel>
        <StackPanel   Grid.Row="6"  Orientation="Horizontal" HorizontalAlignment="Center" Margin="3,0,4,0" Width="121" Grid.Column="1">

            <TextBox x:Name="_LaserSNL1"
                        Width="111"
                        Height="25"
                        FontSize="14"
                        FontWeight="SemiBold"
                        VerticalContentAlignment="Center"
