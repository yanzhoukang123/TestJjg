            <TextBox x:Name="_SensorML1"
                        Width="111"
                        Height="25"
                        FontSize="14"
                        FontWeight="SemiBold"
                        VerticalContentAlignment="Center"
                        BorderBrush="{x:Null}" BorderThickness="0"
                        IsReadOnly="{Binding Path=IsReadOnly}"
                        Text="{Binding Path=SensorML1, Mode=TwoWay, TargetNullValue={x:Static sys:String.Empty}}" Margin="0,2"/>
        </StackPanel>
        <StackPanel Grid.Column="3"   Grid.Row="2"  Orientation="Horizontal"  HorizontalAlignment="Center" Margin="7,0,3,0" Width="114">
            <TextBox x:Name="_SensorMR1"
