                        BorderBrush="{x:Null}" BorderThickness="0"
                        Text="{Binding Path=SensorMR2, Mode=TwoWay, TargetNullValue={x:Static sys:String.Empty}}" Margin="0,2"/>
        </StackPanel>
        <!--SensorSN-->
